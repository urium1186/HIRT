    <Grid>
        <TextBox Text="{Binding DecompiledStr}" HorizontalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" VerticalScrollBarVisibility="Auto" IsReadOnly="True" ></TextBox>
